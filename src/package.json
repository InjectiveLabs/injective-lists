{
  "name": "injective-lists",
  "version": "1.0.0",
  "description": "A way to store different lists in JSON format.",
  "main": "index.js",
  "scripts": {
    "//fetch": "========== fetch & cache data ==========",
    "fetch:cache:data": "ts-node ./ts-scripts/fetch/cacheData",
    "fetch:swap:routes": "ts-node ./ts-scripts/fetch/swapRoutes",
    "fetch:wallets:ofac": "ts-node ./ts-scripts/fetch/ofacList",
    "fetch:mito:vaults": "ts-node ./ts-scripts/fetch/mitoVaults",
    "fetch:wasm:codeIds": "ts-node ./ts-scripts/fetch/wasmCodeIds",
    "//generate": "========== generate/format data from cache data ==========",
    "generate:configs": "ts-node ./ts-scripts/generate/configs",
    "generate:slugs": "ts-node ./ts-scripts/generate/marketSlugs",
    "generate:marketMap": "ts-node ./ts-scripts/generate/marketMap",
    "generate:tokens:mito": "ts-node ./ts-scripts/generate/mitoTokens",
    "generate:swap:routes": "ts-node ./ts-scripts/generate/swapRoutes",
    "generate:restriction": "ts-node ./ts-scripts/generate/restrictionList",
    "generate:wasm:query": "ts-node ./ts-scripts/generate/wasmQueryMessages",
    "generate:verifiedDenoms": "ts-node ./ts-scripts/generate/verifiedDenoms",
    "generate:wasm:execute": "ts-node ./ts-scripts/generate/wasmExecuteMessages",
    "generate:tokens:list": "ts-node ./ts-scripts/generate/tokens",
    "generate:tokens:static": "ts-node ./ts-scripts/generate/tokens/staticTokens",
    "generate:tokens:supply": "ts-node ./ts-scripts/generate/tokens/supplyTokens",
    "generate:tokens:factory": "ts-node ./ts-scripts/generate/tokens/factoryTokens",
    "generate:tokens:verified": "ts-node ./ts-scripts/generate/tokens/verifiedTokens",
    "generate:tokens:external": "ts-node ./ts-scripts/generate/tokens/externalTokens",
    "//upload": "========== upload images to cloudflare ==========",
    "upload:tokens:images": "ts-node ./ts-scripts/upload/tokenImages",
    "upload:validators:images": "ts-node ./ts-scripts/upload/validatorImages",
    "//validation": "========== validation scripts ==========",
    "check:duplicates": "ts-node ./duplicateCheck",
    "//combined": "========== combined scripts ==========",
    "generate:validators": "pnpm upload:validators:images && ts-node ./ts-scripts/generate/validators",
    "generate:wasm": "pnpm fetch:wasm:codeIds && pnpm generate:wasm:query && pnpm generate:wasm:execute",
    "generate:tokens": "pnpm fetch:cache:data && pnpm generate:marketMap && pnpm fetch:mito:vaults && pnpm generate:tokens:static && pnpm generate:tokens:factory && pnpm generate:tokens:external && pnpm generate:tokens:mito && pnpm generate:tokens:supply && pnpm upload:tokens:images && ts-node ./ts-scripts/generate/tokens && pnpm generate:tokens:verified",
    "//combinedClean": "========== combined scripts + cache cleaning ==========",
    "generate:tokens:flush": "ts-node ./flushCw20Cache && pnpm tokens:generate",
    "generate:wasm:flush": "pnpm fetch:wasm:codeIds && pnpm generate:wasm:query -f && pnpm generate:wasm:execute -f",
    "generate:tokens:evm": "ts-node ./ts-scripts/generate/evm",
    "postinstall": "pnpm upgrade '@injectivelabs/*' --latest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@injectivelabs/networks": "1.16.35",
    "@injectivelabs/sdk-ts": "1.16.35",
    "@injectivelabs/ts-types": "1.16.35",
    "@injectivelabs/utils": "1.16.35",
    "@types/node": "^20.11.30",
    "alchemy-sdk": "^2.6.3",
    "axios": "^1.12.2",
    "dotenv": "^16.4.5",
    "form-data": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}
