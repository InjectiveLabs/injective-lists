{
  "name": "injective-lists",
  "version": "1.0.0",
  "description": "A way to store different lists in JSON format.",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "//fetch": "========== fetch & cache data ==========",
    "fetch:cache:data": "tsx ./ts-scripts/fetch/cacheData.ts",
    "fetch:swap:routes": "tsx ./ts-scripts/fetch/swapRoutes.ts",
    "fetch:wallets:ofac": "tsx ./ts-scripts/fetch/ofacList.ts",
    "fetch:mito:vaults": "tsx ./ts-scripts/fetch/mitoVaults.ts",
    "fetch:wasm:codeIds": "tsx ./ts-scripts/fetch/wasmCodeIds.ts",
    "//generate": "========== generate/format data from cache data ==========",
    "generate:configs": "tsx ./ts-scripts/generate/configs.ts",
    "generate:slugs": "tsx ./ts-scripts/generate/marketSlugs.ts",
    "generate:marketMap": "tsx ./ts-scripts/generate/marketMap.ts",
    "generate:tokens:mito": "tsx ./ts-scripts/generate/mitoTokens.ts",
    "generate:swap:routes": "tsx ./ts-scripts/generate/swapRoutes.ts",
    "generate:restriction": "tsx ./ts-scripts/generate/restrictionList.ts",
    "generate:wasm:query": "tsx ./ts-scripts/generate/wasmQueryMessages.ts",
    "generate:verifiedDenoms": "tsx ./ts-scripts/generate/verifiedDenoms.ts",
    "generate:wasm:execute": "tsx ./ts-scripts/generate/wasmExecuteMessages.ts",
    "generate:tokens:list": "tsx ./ts-scripts/generate/tokens.ts",
    "generate:tokens:static": "tsx ./ts-scripts/generate/tokens/staticTokens.ts",
    "generate:tokens:supply": "tsx ./ts-scripts/generate/tokens/supplyTokens.ts",
    "generate:tokens:factory": "tsx ./ts-scripts/generate/tokens/factoryTokens.ts",
    "generate:tokens:verified": "tsx ./ts-scripts/generate/tokens/verifiedTokens.ts",
    "generate:tokens:external": "tsx ./ts-scripts/generate/tokens/externalTokens.ts",
    "//upload": "========== upload images to cloudflare ==========",
    "upload:tokens:images": "tsx ./ts-scripts/upload/tokenImages.ts",
    "upload:validators:images": "tsx ./ts-scripts/upload/validatorImages.ts",
    "//validation": "========== validation scripts ==========",
    "check:duplicates": "tsx ./duplicateCheck.ts",
    "//combined": "========== combined scripts ==========",
    "generate:validators": "pnpm upload:validators:images && tsx ./ts-scripts/generate/validators.ts",
    "generate:wasm": "pnpm fetch:wasm:codeIds && pnpm generate:wasm:query && pnpm generate:wasm:execute",
    "generate:tokens": "pnpm fetch:cache:data && pnpm generate:marketMap && pnpm fetch:mito:vaults && pnpm generate:tokens:static && pnpm generate:tokens:factory && pnpm generate:tokens:external && pnpm generate:tokens:mito && pnpm generate:tokens:supply && pnpm upload:tokens:images && pnpm generate:tokens:list && pnpm generate:tokens:verified",
    "//combinedClean": "========== combined scripts + cache cleaning ==========",
    "generate:tokens:flush": "tsx ./flushCw20Cache.ts && pnpm tokens:generate",
    "generate:wasm:flush": "pnpm fetch:wasm:codeIds && pnpm generate:wasm:query -f && pnpm generate:wasm:execute -f",
    "generate:tokens:evm": "tsx ./ts-scripts/generate/evm.ts",
    "postinstall": "pnpm upgrade '@injectivelabs/*' --latest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@injectivelabs/networks": "1.17.6",
    "@injectivelabs/sdk-ts": "1.17.6",
    "@injectivelabs/ts-types": "1.17.6",
    "@injectivelabs/utils": "1.17.6",
    "@types/node": "^20.11.30",
    "alchemy-sdk": "^2.6.3",
    "axios": "^1.12.2",
    "dotenv": "^16.4.5",
    "form-data": "^4.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.4.3"
  }
}
